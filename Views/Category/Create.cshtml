@model MovieMVC.Models.Category

@{
    ViewData["Title"] = "Category Movie";
}
<div class="card">
    <div class="card-header">Add Category Movie</div>
    <div class="card-body">
        <form method="post" asp-action="create">
            @Html.AntiForgeryToken()
            <div class="mb-3">
                <label class="form-label" asp-for="@Model.CategoryName"></label>
                <input type="text" class="form-control" asp-for="@Model.CategoryName"/>
            </div>
            <a class="btn btn-danger" asp-action="index">Back</a>
            <button type="submit" class="btn btn-primary">Save</button>
        </form>
    </div>
</div>
